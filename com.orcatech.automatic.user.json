{
  "type": "record",
  "namespace": "com.orcatech.automatic",
  "doc": "User is the object representing an Automatic account holder. In practice this represents an owner or driver of a vehicle. A user may have many vehicles (and a vehicle may have many users).",
  "name": "user",
  "fields": [{
      "name": "url",
      "type": "string",
      "doc": "User URI"
    },
    {
      "name": "id",
      "type": "string",
      "doc": "Unique identifier for the user given by Automatic"
    },
    {
      "name": "userName",
      "type": "string",
      "doc": "[scope:public scope:user:profile]"
    },
    {
      "name": "firstName",
      "type": "string",
      "doc": "User's first name [scope:public scope:user:profile]"
    },
    {
      "name": "lastName",
      "type": "string",
      "doc": "User's last name [scope:public scope:user:profile]"
    },
    {
      "name": "email",
      "type": "string",
      "doc": "User's email address [scope:public scope:user:profile]"
    }
  ]
}