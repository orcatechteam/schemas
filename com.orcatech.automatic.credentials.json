{
  "type": "record",
  "namespace": "com.orcatech.automatic",
  "doc": "Credentials for accessing automatic data",
  "name": "credentials",
  "fields": [{
      "name": "token",
      "type": "string",
      "doc": "Access token"
    },
    {
      "name": "refresh",
      "type": "string",
      "doc": "Refresh token for generating a new access token"
    },
    {
      "name": "expiration",
      "type": {
        "type": "long",
        "logicalType": "timestamp-millis"
      },
      "doc": "Estimated time when the token will expire"
    }
  ]
}